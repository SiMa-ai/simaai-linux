			0x0 0x40c5034 0x0 0xFCC>;
		reg-names = "csi-reg0", "csi-reg1", "glue-reg0", "glue-reg1";
		interrupts = <GIC_SPI 78 IRQ_TYPE_LEVEL_HIGH>;
		phys = <&mipi_dphy_rx1 0>;
		phy-names = "snps-dphy-rx";
		status = "disabled";
		isp-connected = <0>;
		output-type = <2>;
		ipi-mode = <0>;
		ipi-color-mode = <0>;
		ipi-auto-flush = <1>;
		virtual-channel = <1>;
	};

	mipi_dphy_rx2: dphy@40c6040 {
